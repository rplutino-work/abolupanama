{
  "flex-layout.row#block-4": {
    "title": "Ofertas del Mes - Productos en Descuento",
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": false,
      "preventVerticalStretch": false,
      "fullWidth": true,
      "colSizing": "auto",
      "blockClass": [
        "home-block-4"
      ]
    },
    "children": [
      // "rich-text#home-block-4",
      "link#home-block-4",
      "list-context.product-list#block-4"
    ]
  },
  "link#home-block-4": {
    "title": "Enlace a Ofertas del Mes",
    "props": {
      "href": "/166?map=productClusterIds",
      "label": "Ofertas del mes",
      "blockClass": "home-block-4"
    }
  },
  "rich-text#home-block-4": {
    "props": {
      "text": "**Ofertas del mes**",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": "home-block-4"
    }
  },
  "list-context.product-list#block-4": {
    "title": "Lista de Productos en Oferta (Colecci√≥n 166)",
    "props": {
      "hideUnavailableItems": false,
      "maxItems": 70,
      "collection": "166"
    },
    "blocks": [
      "product-summary.shelf#product-full"
    ],
    "children": [
      "slider-layout#block-4"
    ]
  },
  "slider-layout#block-4": {
    "title": "Slider de Ofertas (4 columnas desktop)",
    "props": {
      "fullWidth": true,
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 1
      },
      "slideTransition": {
        "speed": 500,
        "delay": 0,
        "timing": "ease-in-out"
      },
      "infinite": true,
      "blockClass": [
        "block-4"
      ],
      "centerMode": {
        "desktop": "disabled",
        "tablet": "disabled",
        "phone": "to-the-left"
      },
      "showNavigationArrows": "always",
      "showPaginationDots": "never"
    }
  }
}